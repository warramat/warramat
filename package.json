    "lint": "eslint ."
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/jest/mock.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
