    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "husky": "^3.0.1",
    "jest-expo": "^34.0.0",
    "lint-staged": "^9.2.1",
    "node-fetch": "^3.1.1",
    "prettier": "^1.18.2",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.8.6"
  },
  "dependencies": {
